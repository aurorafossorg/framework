	"version": "0.2.0",
	"configurations": [
		{
			"name": "examples_gui-simple_nativewindow",
			"type": "gdb",
			"request": "launch",
			"target": "./examples/gui-simple_nativewindow/.out/bin/aurorafw-examples_gui-simple_nativewindow",
			"cwd": "${workspaceRoot}"
		},
		{
			"name": "legacy_examples_gengine",
			"type": "gdb",
			"request": "launch",
			"target": "./.build/examples/legacy/gengine/aurorafw-gengine-legacy-example",
			"cwd": "${workspaceRoot}"
		},
		{
			"name": "examples_metadata-detect_files",
			"type": "gdb",
			"request": "launch",
			"target": "./examples/metadata-detect_files/.out/bin/aurorafw-examples_metadata-detect_files",
			"arguments": "~/JPEG_example_JPG_RIP_100.jpg",
			"cwd": "${workspaceRoot}"
		}
	]
}